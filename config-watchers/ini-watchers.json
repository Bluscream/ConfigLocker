{
  "watchers": [
    {
      "name": "INI Config Example",
      "description": "Watches INI configuration files and merges changes",
      "type": "INI",
      "enabled": true,
      "inputs": [
        "./examples/input1.ini",
        "./examples/input2.ini"
      ],
      "output": "./examples/output.ini",
      "checkonchange": true,
      "checkonstartup": true,
      "checkevery": "00:30:00"
    }
  ]
} 